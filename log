   dh_makeshlibs -O--parallel
	rm -f debian/libhts1/DEBIAN/shlibs
	install -d debian/libhts1/DEBIAN
	echo 'libhts 1 libhts1' >>debian/libhts1/DEBIAN/shlibs
	echo "# Automatically added by dh_makeshlibs">> debian/libhts1.postinst.debhelper
	sed "" /usr/share/debhelper/autoscripts/postinst-makeshlibs >> debian/libhts1.postinst.debhelper
	echo '# End automatically added section' >> debian/libhts1.postinst.debhelper
	echo "# Automatically added by dh_makeshlibs">> debian/libhts1.postrm.debhelper
	sed "" /usr/share/debhelper/autoscripts/postrm-makeshlibs >> debian/libhts1.postrm.debhelper
	echo '# End automatically added section' >> debian/libhts1.postrm.debhelper
	chmod 0644 debian/libhts1/DEBIAN/shlibs
	chown 0:0 debian/libhts1/DEBIAN/shlibs
	dpkg-gensymbols -plibhts1 -Idebian/libhts1.symbols -Pdebian/libhts1 "-edebian/libhts1/usr/lib/x86_64-linux-gnu/libhts.so.1.2.1-1\
"
	rm -f debian/libhts-dev/DEBIAN/shlibs
	rm -f debian/htslib-test/DEBIAN/shlibs
	rm -f debian/tabix/DEBIAN/shlibs
   dh_shlibdeps -O--parallel
	dpkg-shlibdeps -Tdebian/libhts1.substvars debian/libhts1/usr/lib/x86_64-linux-gnu/libhts.so.1.2.1-1
	install -d debian/libhts-dev/DEBIAN
	install -d debian/htslib-test/DEBIAN
	install -d debian/tabix/DEBIAN
	dpkg-shlibdeps -Tdebian/tabix.substvars debian/tabix/usr/bin/tabix debian/tabix/usr/bin/bgzip
   dh_installdeb -O--parallel
	printf '#!/bin/sh
set -e
' > debian/libhts1/DEBIAN/postinst
	cat debian/libhts1.postinst.debhelper >> debian/libhts1/DEBIAN/postinst
	chown 0:0 debian/libhts1/DEBIAN/postinst
	chmod 0755 debian/libhts1/DEBIAN/postinst
	printf '#!/bin/sh
set -e
' > debian/libhts1/DEBIAN/postrm
	cat debian/libhts1.postrm.debhelper >> debian/libhts1/DEBIAN/postrm
	chown 0:0 debian/libhts1/DEBIAN/postrm
	chmod 0755 debian/libhts1/DEBIAN/postrm
   dh_gencontrol -O--parallel
	echo misc:Depends= >> debian/libhts1.substvars
	echo misc:Pre-Depends= >> debian/libhts1.substvars
	dpkg-gencontrol -plibhts1 -ldebian/changelog -Tdebian/libhts1.substvars -Pdebian/libhts1
	chmod 0644 debian/libhts1/DEBIAN/control
	chown 0:0 debian/libhts1/DEBIAN/control
