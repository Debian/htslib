	echo misc:Pre-Depends= >> debian/libhts-dev.substvars
	dpkg-gencontrol -plibhts-dev -ldebian/changelog -Tdebian/libhts-dev.substvars -Pdebian/libhts-dev
	chmod 0644 debian/libhts-dev/DEBIAN/control
	chown 0:0 debian/libhts-dev/DEBIAN/control
