	echo misc:Pre-Depends= >> debian/htslib-test.substvars
	dpkg-gencontrol -phtslib-test -ldebian/changelog -Tdebian/htslib-test.substvars -Pdebian/htslib-test
	chmod 0644 debian/htslib-test/DEBIAN/control
	chown 0:0 debian/htslib-test/DEBIAN/control
	echo misc:Depends= >> debian/tabix.substvars
	echo misc:Pre-Depends= >> debian/tabix.substvars
