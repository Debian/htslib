   dh_fixperms -O--parallel
	find debian/libhts1  -print0 2>/dev/null | xargs -0r chown --no-dereference 0:0
	find debian/libhts1 ! -type l  -print0 2>/dev/null | xargs -0r chmod go=rX,u+rw,a-s
