	chmod 0644 debian/tabix/DEBIAN/control
	chown 0:0 debian/tabix/DEBIAN/control
   dh_md5sums -O--parallel
	(cd debian/libhts1 >/dev/null ; find . -type f  ! -regex './DEBIAN/.*' -printf '%P\0' | LC_ALL=C sort -z | xargs -r0 md5sum > DEBIAN/md5sums) >/dev/null
	chmod 0644 debian/libhts1/DEBIAN/md5sums
	chown 0:0 debian/libhts1/DEBIAN/md5sums
	(cd debian/libhts-dev >/dev/null ; find . -type f  ! -regex './DEBIAN/.*' -printf '%P\0' | LC_ALL=C sort -z | xargs -r0 md5sum > DEBIAN/md5sums) >/dev/null
	chmod 0644 debian/libhts-dev/DEBIAN/md5sums
	chown 0:0 debian/libhts-dev/DEBIAN/md5sums
	(cd debian/htslib-test >/dev/null ; find . -type f  ! -regex './DEBIAN/.*' -printf '%P\0' | LC_ALL=C sort -z | xargs -r0 md5sum > DEBIAN/md5sums) >/dev/null
	chmod 0644 debian/htslib-test/DEBIAN/md5sums
	chown 0:0 debian/htslib-test/DEBIAN/md5sums
	(cd debian/tabix >/dev/null ; find . -type f  ! -regex './DEBIAN/.*' -printf '%P\0' | LC_ALL=C sort -z | xargs -r0 md5sum > DEBIAN/md5sums) >/dev/null
	chmod 0644 debian/tabix/DEBIAN/md5sums
	chown 0:0 debian/tabix/DEBIAN/md5sums
