   dh_installman
	man --recode UTF-8 ./vcf\.5 > vcf\.5\.new
	chmod 644 vcf.5.new
	mv -f vcf.5.new vcf.5
	man --recode UTF-8 ./faidx\.5 > faidx\.5\.new
	chmod 644 faidx.5.new
	mv -f faidx.5.new faidx.5
